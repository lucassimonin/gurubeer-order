<!DOCTYPE HTML>
<html class="gt-ie8 gt-ie9 not-ie" lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{% endblock %} | {{ 'admin.adminstration.label'|trans({}, 'app') }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
    <link rel="icon" type="image/png" href="{{ asset('images/admin/favicon.ico') }}"  />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    {% block stylesheets %}
        <link href="{{ asset('build/admin.css') }}" rel="stylesheet" type="text/css">
    {% endblock %}
</head>
<body id="{% block bodyId %}{% endblock %}">
    {% trans_default_domain 'app' %}
    {% block headbar %}
        {% include "admin/parts/headbar.html.twig" %}
    {% endblock %}
    <div class="container-fluid">
        <main role="main" class="ml-sm-auto pt-3">
            {% block breadcrumb %}{{ wo_render_breadcrumbs({ viewTemplate: "admin/parts/breadcrumbs.html.twig" }) }}{% endblock %}
            {% include 'admin/parts/flashes.html.twig' %}

            <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 border-bottom">
                <h1 class="h2">{% block mainTitle %}{% endblock %}</h1>
                {% block options %}{% endblock %}
            </div>
            {% block content %}{% endblock %}
        </main>
    </div>
    {% block javascripts %}
        <script>
            var add_label = "{{ 'admin.common.add'|trans({}, 'app') }}";
            var delete_label = "{{ 'admin.common.delete'|trans({}, 'app') }}";
        </script>
        <script src="{{ asset('build/admin.js') }}"></script>
    {% endblock %}
</body>
</html>
